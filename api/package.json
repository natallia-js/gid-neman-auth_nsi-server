    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.15",
    "ramda": "^0.27.1",
    "sequelize": "^6.5.1",
    "tedious": "^11.0.6",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
